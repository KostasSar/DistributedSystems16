package beans;

import java.io.Serializable;

public class Vehicle implements Serializable {
	private static final long serialVersionUID = -1355807851822025659L;
	private String Model;
	private String License_Plate;
	private String Fuel_Type;
	private int Release_Year;
	private String Vehicle_Condition;
	private int Store_id;
	private int TRN;

	public Vehicle(String model, String lp, String ft,int rl, String vp,int si, int trn ) {
		this.Model = model;
		this.License_Plate = lp;
		this.Fuel_Type = ft;
		this.Release_Year = rl;
		this.Vehicle_Condition = vp;
		this.Store_id = si;
		this.TRN = trn;
		
	}

	public void setModel(String model) {
		this.Model = model;
	}

	public void setLicensePlate(String lp) {
		this.License_Plate = lp;
	}

	public void setFuelType(String ft) {
		this.Fuel_Type = ft;
	}

	
	public void setReleaseYear(int rl) {
		this.Release_Year = rl;
	}
	
	public void setVehicleCondition(String vp) {
		this.Vehicle_Condition = vp;
	}
	
	
	public void setStore_ID(int store_id) {
		this.Store_id = store_id;
	}
	
	
	public void setTRN(int TRN) {
		this.TRN = TRN;
	}
	
	
	public String getModel() {
		return Model;
	}

	public String getLicensePlate() {
		return License_Plate;
	}

	public String getFuelType() {
		return Fuel_Type;
	}

	public int getReleaseYear() {
		return Release_Year;
	}

	
	public String getVehicleCondition() {
		return Vehicle_Condition;
	}
	
	public int getStoreID() {
		return Store_id;
	}
	
	public int getTRN() {
		return TRN;
	}
	
	
	@Override
	public String toString() {
		return "Model=" + this.Model + ", License Plate =" + this.License_Plate + ", Fuel Type =" + this.Fuel_Type + ", Release Year = " + this.Release_Year + " Vehicle Condition = " + this.Vehicle_Condition + " Store ID " + this.Store_id + " Owner TRN " + this.TRN ;
	}
}
